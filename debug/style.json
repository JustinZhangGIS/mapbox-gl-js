{
  "version": 7,
  "constants": {
    "@constant": "red"
  },
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "url": "mapbox://mapbox.mapbox-terrain-v2"
    }
  },
  "layers": [{
    "id": "background",
    "type": "background",
    "paint": {
      "background-color": "white"
    }
  }, {
    "id": "countour",
    "type": "line",
    "source": "mapbox",
    "source-layer": "contour",
    "paint": {
      "line-width": 2,
      "line-color": {
        "property": "ele",
        "domain": [0, 2000, 4000],
        "range": ["green", "brown", "grey"]
      }
    }
  }]
}
